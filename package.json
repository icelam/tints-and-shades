  "scripts": {
    "start": "webpack-dev-server --config webpack/webpack.dev.conf.js",
    "start:electron": "ELECTRON_START_URL=http://localhost:8888 electron --inspect=5858 .",
    "build": "webpack --config webpack/webpack.prod.conf.js  --colors",
    "lint": "eslint --ext .js,.ts src",
    "lint:lit-analyzer": "lit-analyzer",
    "package:mac": "yarn clean:package && yarn build && electron-builder build --mac --config electron-builder.config.json",
    "package:linux": "yarn clean:package && yarn build && electron-builder build --linux --config electron-builder.config.json",
    "package:windows": "yarn clean:package && yarn build && electron-builder build --win --x64 --ia32 --config electron-builder.config.json",
    "package": "yarn clean:package && yarn build && electron-builder build -mwl --x64 --ia32 --config electron-builder.config.json",
    "clean:package": "rimraf ./build-packages",
    "analyze:web": "wca analyze \"src/**/*.ts\" --outFile wca-report.json",
    "first-release": "npx standard-version --commit-all --tag-prefix v --release-as 1.0.0 --infile CHANGELOG.md",
    "release": "npx standard-version --commit-all --tag-prefix v --infile CHANGELOG.md",
    "bump-readme-download-link": "node scripts/bumpReadmeDownloadLink.js && git add README.md",
    "bump-version-constant": "node scripts/bumpVersionConstant.js && git add src/constants.ts",
    "extract-latest-change-log": "node scripts/extractLatestChangeLog.js",
    "abi:electron": "electron -a",
    "abi:node": "node -e \"console.log(process.versions.modules)\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie <= 9",
      "not op_mini all",
      "ie 10",
      "ie 11"
    ],
    "development": [
      ">0.2%",
      "not dead",
      "not ie <= 9",
      "not op_mini all",
      "ie 10",
      "ie 11"
    ]
  }
}
